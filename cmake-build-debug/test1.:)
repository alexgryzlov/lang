linear(b, c) {
    if (b == 0) {
        if (c == 0) {
            return inf();
        }
        return nosol();
    }
    return -(c / b);
}

nosol() {
    return -42;
}

inf() {
    return 42;
}

discr(a, b, c) {
    return ((b * b) - ((4 * a) * c);
}


main() {
    data a, b, c, x1, x2, d;
    in a, b, c;
    if (a == 0) {
        out linear(b, c);
    }
    if (0 < a) {
        d = sqrt(discr(a, b, c));
        if (d < 0) {
            out nosol();
        }
        x1 = ((-b) + d) / (2 * a);
        x2 = ((-b) - d) / (2 * a);
        if (d == 0) {
            out x1;
        }
        if (0 < d) {
            out x1, x2;
        }
    }
}